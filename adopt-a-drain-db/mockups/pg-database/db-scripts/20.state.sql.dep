\c api_db
select '##### EXTENSIONS';
select '#####  ROLES';
SELECT * FROM pg_roles;
select '#####  SCHEMA';
select nspname from pg_catalog.pg_namespace;
select '#####  TABLES';
SELECT *
  FROM information_schema.tables
  WHERE table_schema IN ('api_schema','public');
select '#####  FUNCTIONS';
-- SELECT routine_name
--  FROM information_schema.routines
--    WHERE routine_type='FUNCTION' AND specific_schema='api_schema';
-- routine_schema | routine_name
SELECT routine_schema, routine_name
  FROM information_schema.routines
    WHERE routine_type='FUNCTION' AND specific_schema in ('api_schema');
select '#####  OUT';
